<?xml version="1.0" encoding="utf-8" ?>
<Defs>



	<ThingDef ParentName="SK_DeciduousPlantDef">
		<defName>Plant_Pickles</defName>
		<label>Pickles Plant</label>
		<description>The cucumber plant is a creeping vine that takes root in the ground and grows up trellises or other support structures, twining around the supports with thin, spiral-shaped tendrils. Vegetables can even be grown in a soilless environment using hydroponics.</description>
		<graphicData>
			<texPath>Things/Plant/Pickles</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<statBases>
			<Beauty>2</Beauty>
			<BeautyOutdoors>2</BeautyOutdoors>
			<Nutrition>0.4</Nutrition>
		</statBases>
		<plant>
			<maxMeshCount>4</maxMeshCount>
			<sowMinSkill>6</sowMinSkill>
			<sowResearchPrerequisites>
				<li>Food_Plants_A1</li>
			</sowResearchPrerequisites>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<immatureGraphicPath>Things/Plant/Pickles_Immature</immatureGraphicPath>
			<harvestedThingDef>RawPickles</harvestedThingDef>
			<harvestYield>12</harvestYield>
			<topWindExposure>0.1</topWindExposure>
			<growDays>3.3</growDays>
			<fertilitySensitivity>1</fertilitySensitivity>
			<fertilityMin>0.4</fertilityMin>
			<visualSizeRange>
				<min>0.2</min>
				<max>0.9</max>
			</visualSizeRange>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
			<humanFoodPlant>true</humanFoodPlant>
		</plant>
	</ThingDef>


</Defs>